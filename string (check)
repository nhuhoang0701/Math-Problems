// Check if a string of parentheses is properly nested
bool check_parentheses(char *s) {
    int count =0;
    int len = strlen(s);
    for (int i =0; i<len; i++) {
        if (s[i]=='(') count++;
        if (s[i]==')') {
            if (count>0) count--;
            else return false;
        }
    }
    return (count==0)?true:false;
}
