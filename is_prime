bool is_prime(int n) // Check if an integer a prime or not using division of 6k-1 and 6k+1
{
	if (n == 2 || n == 3) return true;
	if (n == 1 || n % 2 == 0 || n % 3 == 0) return false;
	int sqrt_n = sqrt(n);
	int k = 1;
	while ((6 * k - 1 < sqrt_n) || (6 * k + 1 < sqrt_n))
	{
		if (n % (6 * k - 1) == 0 || n % (6 * k + 1) == 0) return false;
	}
	return true;
}
